    {
      "_docs": "WELCOME: First intent triggered. Gets consent before proceeding with survey.",
      "name": "WelcomeIntent",
      "description": "Initial greeting and consent to proceed with census survey",
      "sampleUtterances": [
        { "utterance": "Hello" },
        { "utterance": "Hi" },
        { "utterance": "Start census" },
        { "utterance": "Begin survey" },
        { "utterance": "I want to complete my census" },
        { "utterance": "Help me with census" },
        { "utterance": "Census survey" },
        { "utterance": "I'm calling about the census" },
        { "utterance": "Start" }
      ],
      "fulfillmentCodeHook": {
        "enabled": true,
        "postFulfillmentStatusSpecification": {
          "successResponse": {
            "messageGroups": [
              {
                "message": {
                  "plainTextMessage": {
                    "value": "Thank you for agreeing to participate. Let me verify your address on file."
                  }
                }
              }
            ],
            "allowInterrupt": false
          }
        }
      },
      "initialResponseSetting": {
        "initialResponse": {
          "messageGroups": [
            {
              "message": {
                "plainTextMessage": {
                  "value": "Hello! Thank you for taking the time to speak with me today. I'm an automated assistant calling on behalf of the U.S. Census Bureau to help complete your household's census information. Your responses are protected by federal law under Title 13 and are kept strictly confidential. They will only be used for statistical purposes."
                }
              }
            }
          ],
          "allowInterrupt": false
        }
      },
      "slots": [
        {
          "name": "ConsentToProceed",
          "slotTypeName": "YesNoType",
          "description": "Whether the constituent consents to proceed with the survey",
          "valueElicitationSetting": {
            "slotConstraint": "Required",
            "promptSpecification": {
              "messageGroups": [
                {
                  "message": {
                    "plainTextMessage": {
                      "value": "May I proceed with a few questions about your household?"
                    }
                  }
                }
              ],
              "maxRetries": 3,
              "allowInterrupt": true
            }
          }
        }
      ],
      "slotPriorities": [
        { "priority": 1, "slotName": "ConsentToProceed" }
      ]
    },
